        <addForeignKeyConstraint baseColumnNames="mashbill_id"
                                 baseTableName="batch"
                                 constraintName="fk_batch_mashbill_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mashbill"/>

    </changeSet>
</databaseChangeLog>
